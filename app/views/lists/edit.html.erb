<h1>Editing List</h1>
<%= form_for([@group, @group.lists.build]) do |f| %>
  <p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :amount %>
    <%= f.number_field :amount %>
  </p>
  <p>
    <%= f.submit %>
  </p>

<% end %>
<%= link_to 'Back', groups_path %>
